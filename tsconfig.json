{
  "compilerOptions": {
    "rootDir": "./src",
    "target": "es6",
    "lib": ["es2017", "es6", "dom"],
    "moduleResolution": "Node16",
    "strict": true,
    "useUnknownInCatchVariables": false,
    "strictNullChecks": false,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules"],
  "include": ["./src"]
}
